{shotspot3=x206 y283 a219 b296 u'ptviewer:startAutoPan(0.5,0,1)' p i'./img/right.gif'}
{shotspot4=x193 y270 a206 b283 u'ptviewer:startAutoPan(0,0.5,1)' p i'./img/up.gif'}
{shotspot5=x193 y296 a206 b309 u'ptviewer:startAutoPan(0,-0.5,1)' p i'./img/down.gif'}
{shotspot6=x193 y283 a206 b296 u'ptviewer:stopAutoPan()'  p i'./img/stop.gif'}

